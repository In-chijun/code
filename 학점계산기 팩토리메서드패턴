var CreditCalculator = (function() {
    function CreditCalculator() {};
    var sections = [];
    CreditCalculator.prototype.create = function() {
        // console.log(`create 메서드입니다.`);
    };
    CreditCalculator.prototype.getSctions = function() {
        return sections;
    };
    CreditCalculator.prototype.addSctions = function(section) {
        return sections.push(section);
    };
    return CreditCalculator;
})();

var Credit3 = new CreditCalculator();
Credit3.prototype.create = function(){
    CreditCalculator.addSctions(new InputMaxCredit());
    CreditCalculator.addSctions(new InputSubjectCount());
    CreditCalculator.addSctions(new InputSubject());
    CreditCalculator.addSctions(new InputScore());
    CreditCalculator.addSctions(new InputCredit());
    CreditCalculator.addSctions(new ChangeTo4_5());
}
var credit = new Credit3();
credit.create()
console.log(credit.getSctions())

var class1 = (function() {
    function class1() {}
    class1.prototype.interface = function() {
        return 'interface';
    };
    return class1;
})();

var person;
person.prototype = new class1();

person.interface();
